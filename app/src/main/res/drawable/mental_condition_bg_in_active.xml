<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_checked="true" >

        <shape>
            <padding android:right="38dp" android:left="38dp"/>
            <solid android:color="#2365B5"/>
            <stroke android:width="0.5dp" android:color="#2365B5"/>
            <corners android:radius="15000dp"/>
        </shape>
    </item>
    <item >
        <shape>
            <padding android:right="38dp" android:left="38dp"/>
            <solid android:color="@color/white"/>
            <stroke android:width="0.5dp" android:color="#535359"/>
            <corners android:radius="15000dp"/>
        </shape>
    </item>
</selector>