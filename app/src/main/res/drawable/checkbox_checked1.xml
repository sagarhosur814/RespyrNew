<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/select_primary" />
            <stroke android:color="@color/select_primary" android:width="2dp" />
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item android:id="@android:id/checkbox">
        <bitmap android:src="@drawable/ic_male"
            android:tint="@color/select_primary" />
    </item>
</layer-list>